<?xml version="1.0" encoding="utf-8"?>



<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/startFragment"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#00000000"
    tools:context=".StartFragment"
    android:layout_gravity="bottom"
    >


    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="#00000000">

        <Button
            android:id="@+id/button2"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="106dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="106dp"
            android:layout_marginBottom="100dp"
            android:background="@drawable/button_background"
            android:fontFamily="@font/yantramanav_thin"
            android:text="Favorites"
            android:textAlignment="center"
            android:textColor="@android:color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/autoCompleteTextView2" />

        <AutoCompleteTextView
            android:id="@+id/autoCompleteTextView2"
            android:layout_width="250dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="106dp"
            android:layout_marginTop="110dp"
            android:layout_marginEnd="106dp"
            android:layout_marginBottom="30dp"
            android:fontFamily="@font/yantramanav_thin"
            android:hint="Set destination...."
            android:inputType="text"
            android:textColor="@android:color/white"
            android:textColorHint="@color/quantum_white_100"
            android:textSize="14sp"
            android:theme="@style/Autocomplete"
            app:layout_constraintBottom_toTopOf="@+id/button2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/circle1"
            android:layout_width="7.5dp"
            android:layout_height="7.5dp"
            android:layout_marginStart="110dp"
            android:layout_marginBottom="40dp"
            android:background="@drawable/circle"
            app:layout_constraintBottom_toTopOf="@+id/autoCompleteTextView2"
            app:layout_constraintEnd_toStartOf="@+id/circle2"
            app:layout_constraintStart_toStartOf="@+id/autoCompleteTextView2" />

        <ImageView
            android:id="@+id/circle3"
            android:layout_width="7.5dp"
            android:layout_height="7.5dp"
            android:layout_marginEnd="110dp"
            android:layout_marginBottom="40dp"
            android:background="@drawable/circle"
            app:layout_constraintBottom_toTopOf="@+id/autoCompleteTextView2"
            app:layout_constraintEnd_toEndOf="@+id/autoCompleteTextView2"
            app:layout_constraintStart_toEndOf="@+id/circle2" />

        <ImageView
            android:id="@+id/circle2"
            android:layout_width="7.5dp"
            android:layout_height="7.5dp"
            android:layout_marginStart="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginBottom="40dp"
            android:background="@drawable/circle"
            app:layout_constraintBottom_toTopOf="@+id/autoCompleteTextView2"
            app:layout_constraintEnd_toStartOf="@+id/circle3"
            app:layout_constraintStart_toEndOf="@+id/circle1" />


    </android.support.constraint.ConstraintLayout>
</FrameLayout>